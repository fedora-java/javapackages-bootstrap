<project>
  <licensing>
    <tag>ASL 2.0</tag>
    <file>NOTICE</file>
  </licensing>
  <module>
    <name>maven-artifact</name>
    <subDir>maven-artifact</subDir>
    <dependency>commons-lang</dependency>
    <dependency>plexus-utils</dependency>
    <build>
      <compiler>
        <addSourceRoot>src/main/java</addSourceRoot>
      </compiler>
    </build>
  </module>
  <module>
    <name>maven-builder-support</name>
    <subDir>maven-builder-support</subDir>
    <build>
      <compiler>
        <addSourceRoot>src/main/java</addSourceRoot>
      </compiler>
    </build>
  </module>
  <module>
    <name>maven-compat</name>
    <subDir>maven-compat</subDir>
    <dependency>plexus-utils</dependency>
    <dependency>sisu-plexus</dependency>
    <dependency>maven-artifact</dependency>
    <dependency>maven-wagon</dependency>
    <dependency>maven-repository-metadata</dependency>
    <dependency>plexus-component-annotations</dependency>
    <dependency>maven-core</dependency>
    <dependency>maven-model</dependency>
    <dependency>maven-settings</dependency>
    <dependency>maven-resolver</dependency>
    <dependency>maven-settings-builder</dependency>
    <dependency>maven-model-builder</dependency>
    <dependency>plexus-interpolation</dependency>
    <dependency>maven-builder-support</dependency>
    <build>
      <modello>
        <model>src/main/mdo/paramdoc.mdo</model>
        <version>1.0.0</version>
        <output>java|xpp3-reader|xpp3-writer</output>
      </modello>
      <modello>
        <model>src/main/mdo/profiles.mdo</model>
        <version>1.0.0</version>
        <output>java|xpp3-reader|xpp3-writer</output>
      </modello>
      <compiler>
        <addSourceRoot>src/main/java</addSourceRoot>
        <addResource>src/main/resources</addResource>
      </compiler>
      <cdc/>
    </build>
  </module>
  <module>
    <name>maven-core</name>
    <subDir>maven-core</subDir>
    <dependency>plexus-utils</dependency>
    <dependency>maven-model</dependency>
    <dependency>maven-artifact</dependency>
    <dependency>maven-settings</dependency>
    <dependency>maven-resolver</dependency>
    <dependency>sisu-plexus</dependency>
    <dependency>plexus-classworlds</dependency>
    <dependency>maven-model-builder</dependency>
    <dependency>maven-plugin-api</dependency>
    <dependency>injection-api</dependency>
    <dependency>guice</dependency>
    <dependency>maven-settings</dependency>
    <dependency>sisu-inject</dependency>
    <dependency>maven-settings-builder</dependency>
    <dependency>plexus-component-annotations</dependency>
    <dependency>maven-repository-metadata</dependency>
    <dependency>plexus-interpolation</dependency>
    <dependency>maven-builder-support</dependency>
    <dependency>maven-shared-utils</dependency>
    <dependency>commons-lang</dependency>
    <dependency>maven-resolver-provider</dependency>
    <build>
      <modello>
        <model>src/main/mdo/toolchains.mdo</model>
        <version>1.1.0</version>
        <output>java|xpp3-reader|xpp3-writer</output>
      </modello>
      <compiler>
        <addSourceRoot>src/main/java</addSourceRoot>
        <addResource>src/main/resources</addResource>
      </compiler>
      <cdc>
        <gleanFromXML>src/main/resources/META-INF/plexus/default-bindings.xml</gleanFromXML>
        <gleanFromXML>src/main/resources/META-INF/plexus/artifact-handlers.xml</gleanFromXML>
        <gleanFromXML>src/main/resources/META-INF/plexus/components.xml</gleanFromXML>
      </cdc>
    </build>
  </module>
  <module>
    <name>maven-embedder</name>
    <subDir>maven-embedder</subDir>
    <dependency>plexus-utils</dependency>
    <dependency>maven-shared-utils</dependency>
    <dependency>commons-lang</dependency>
    <dependency>plexus-classworlds</dependency>
    <dependency>maven-core</dependency>
    <dependency>slf4j</dependency>
    <dependency>guice</dependency>
    <dependency>commons-cli</dependency>
    <dependency>injection-api</dependency>
    <dependency>sisu-plexus</dependency>
    <dependency>maven-resolver</dependency>
    <dependency>maven-settings</dependency>
    <dependency>maven-settings-builder</dependency>
    <dependency>maven-model</dependency>
    <dependency>maven-model-builder</dependency>
    <dependency>plexus-sec-dispatcher</dependency>
    <dependency>maven-plugin-api</dependency>
    <dependency>plexus-cipher</dependency>
    <dependency>maven-builder-support</dependency>
    <dependency>maven-artifact</dependency>
    <build>
      <modello>
        <model>src/main/mdo/core-extensions.mdo</model>
        <version>1.0.0</version>
        <output>java|xpp3-reader|xpp3-writer</output>
      </modello>
      <compiler>
        <addSourceRoot>src/main/java</addSourceRoot>
        <excludeSourceClass>LogbackConfiguration</excludeSourceClass>
        <excludeSourceClass>MavenSimpleLogger</excludeSourceClass>
        <addResource>src/main/resources</addResource>
      </compiler>
      <cdc/>
    </build>
  </module>
  <module>
    <name>maven-model-builder</name>
    <subDir>maven-model-builder</subDir>
    <dependency>maven-model</dependency>
    <dependency>injection-api</dependency>
    <dependency>sisu-inject</dependency>
    <dependency>plexus-utils</dependency>
    <dependency>plexus-interpolation</dependency>
    <dependency>maven-builder-support</dependency>
    <dependency>maven-artifact</dependency>
    <build>
      <compiler>
        <addSourceRoot>src/main/java</addSourceRoot>
        <addResource>src/main/resources</addResource>
      </compiler>
      <cdc/>
    </build>
  </module>
  <module>
    <name>maven-model</name>
    <subDir>maven-model</subDir>
    <dependency>plexus-utils</dependency>
    <build>
      <modello>
        <model>src/main/mdo/maven.mdo</model>
        <version>4.0.0</version>
        <output>java|xpp3-reader|xpp3-extended-reader|xpp3-writer</output>
      </modello>
      <compiler>
        <addSourceRoot>src/main/java</addSourceRoot>
      </compiler>
    </build>
  </module>
  <module>
    <name>maven-plugin-api</name>
    <subDir>maven-plugin-api</subDir>
    <dependency>plexus-utils</dependency>
    <dependency>maven-model</dependency>
    <dependency>sisu-plexus</dependency>
    <dependency>maven-artifact</dependency>
    <dependency>plexus-classworlds</dependency>
    <build>
      <modello>
        <model>src/main/mdo/lifecycle.mdo</model>
        <version>1.0.0</version>
        <output>java|xpp3-reader|xpp3-writer</output>
      </modello>
      <compiler>
        <addSourceRoot>src/main/java</addSourceRoot>
      </compiler>
    </build>
  </module>
  <module>
    <name>maven-repository-metadata</name>
    <subDir>maven-repository-metadata</subDir>
    <dependency>plexus-utils</dependency>
    <build>
      <modello>
        <model>src/main/mdo/metadata.mdo</model>
        <version>1.1.0</version>
        <output>java|xpp3-reader|xpp3-writer</output>
      </modello>
      <compiler/>
    </build>
  </module>
  <module>
    <name>maven-resolver-provider</name>
    <subDir>maven-resolver-provider</subDir>
    <dependency>maven-resolver</dependency>
    <dependency>maven-model</dependency>
    <dependency>injection-api</dependency>
    <dependency>maven-artifact</dependency>
    <dependency>plexus-utils</dependency>
    <dependency>maven-repository-metadata</dependency>
    <dependency>maven-model-builder</dependency>
    <dependency>guice</dependency>
    <dependency>maven-builder-support</dependency>
    <build>
      <compiler>
        <addSourceRoot>src/main/java</addSourceRoot>
      </compiler>
      <cdc/>
    </build>
  </module>
  <module>
    <name>maven-settings-builder</name>
    <subDir>maven-settings-builder</subDir>
    <dependency>maven-settings</dependency>
    <dependency>plexus-utils</dependency>
    <dependency>plexus-interpolation</dependency>
    <dependency>maven-builder-support</dependency>
    <dependency>plexus-sec-dispatcher</dependency>
    <dependency>injection-api</dependency>
    <build>
      <compiler>
        <addSourceRoot>src/main/java</addSourceRoot>
      </compiler>
      <cdc/>
    </build>
  </module>
  <module>
    <name>maven-settings</name>
    <subDir>maven-settings</subDir>
    <dependency>plexus-utils</dependency>
    <build>
      <modello>
        <model>src/main/mdo/settings.mdo</model>
        <version>1.1.0</version>
        <output>java|xpp3-reader|xpp3-writer</output>
      </modello>
      <compiler>
        <addSourceRoot>src/main/java</addSourceRoot>
      </compiler>
    </build>
  </module>
</project>
