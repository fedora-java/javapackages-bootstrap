<project>
  <licensing>
    <tag>ASL 2.0</tag>
  </licensing>
  <module>
    <dependency>commons-io</dependency>
    <dependency>maven-archiver</dependency>
    <dependency>maven-model</dependency>
    <dependency>maven-artifact</dependency>
    <dependency>maven-plugin-api</dependency>
    <dependency>maven-plugin-tools</dependency>
    <dependency>maven-core</dependency>
    <dependency>maven-filtering</dependency>
    <dependency>maven-common-artifact-filters</dependency>
    <dependency>velocity-engine</dependency>
    <dependency>plexus-utils</dependency>
    <dependency>plexus-resources</dependency>
    <dependency>maven-model-builder</dependency>
    <dependency>maven-resolver</dependency>
    <build>
      <modello>
        <model>src/main/mdo/remote-resources.mdo</model>
        <version>1.1.0</version>
        <output>java|xpp3-reader|xpp3-writer</output>
      </modello>
      <modello>
        <model>src/main/mdo/supplemental-model.mdo</model>
        <version>1.0.0</version>
