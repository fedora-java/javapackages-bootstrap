<project>
  <licensing>
    <tag>BSD</tag>
  </licensing>
  <module>
    <name>asm-tools-retrofitter</name>
    <build>
      <compiler>
        <release>11</release>
        <addSourceRoot>asm/src/main/java</addSourceRoot>
        <addSourceRoot>tools/retrofitter/src/main/java</addSourceRoot>
      </compiler>
    </build>
  </module>
  <module>
    <subDir>asm</subDir>
    <build>
      <compiler>
        <addSourceRoot>src/main/java</addSourceRoot>
      </compiler>
      <retrofitter/>
    </build>
  </module>
  <module>
    <name>asm-analysis</name>
    <subDir>asm-analysis</subDir>
    <dependency>asm</dependency>
    <dependency>asm-tree</dependency>
    <build>
      <compiler>
        <addSourceRoot>src/main/java</addSourceRoot>
      </compiler>
      <retrofitter/>
    </build>
  </module>
  <module>
    <name>asm-commons</name>
    <subDir>asm-commons</subDir>
    <dependency>asm</dependency>
    <dependency>asm-tree</dependency>
    <build>
      <compiler>
        <addSourceRoot>src/main/java</addSourceRoot>
      </compiler>
      <retrofitter/>
    </build>
  </module>
  <module>
    <name>asm-tree</name>
    <subDir>asm-tree</subDir>
    <dependency>asm</dependency>
    <build>
      <compiler>
        <addSourceRoot>src/main/java</addSourceRoot>
      </compiler>
      <retrofitter/>
    </build>
  </module>
  <module>
    <name>asm-util</name>
    <subDir>asm-util</subDir>
    <dependency>asm</dependency>
    <dependency>asm-analysis</dependency>
    <dependency>asm-tree</dependency>
    <build>
      <compiler>
        <addSourceRoot>src/main/java</addSourceRoot>
      </compiler>
      <retrofitter/>
    </build>
  </module>
</project>
